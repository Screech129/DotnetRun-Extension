{
    "name": "dotnet-run",
    "displayName": "Dotnet Run",
    "description": "Adds ability to use dotnet run from keyboard shortcut",
    "publisher": "Screech129",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:screech129.dotNetRun"
    ],
    "repository": {"url":"https://github.com/Screech129/DotnetRun-Extension/"},
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "screech129.dotNetRun",
                "title": "Dotnet Run"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
